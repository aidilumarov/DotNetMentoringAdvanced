{
  "CartServiceDbOptions": {
    "ConnectionString": "CartService.db"
  },
  "MessageQueue": {
    "Address": "localhost",
    "Queues": {
      "ItemUpdated": "ItemUpdatedQueue"
    }
  },
  "Keycloak": {
    "Authority": "{Keycloak_IP}/auth/realms/{realm_name}",
    "Audience": "{client_id}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
